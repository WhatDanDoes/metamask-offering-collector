<% if (Object.keys(messages).length) { %>
  <article class="messages">
    <% if (messages.success) { %>
        <aside class="alert alert-success">
          <%= messages.success %>
        </aside>
    <% } %>
    <% if (messages.error) { %>
        <aside class="alert alert-error">
          <%= messages.error %>
        </aside>
    <% } %>
    <% if (messages.info) { %>
      <aside class="alert alert-info">
        <%= messages.info %>
      </aside>
    <% } %>
  </article>
<% } %>
