<aside id="messages">
  <% if (Object.keys(messages).length) { %>
    <% if (messages.success) { %>
      <strong class="alert alert-success">
        <%= messages.success %>
      </strong>
    <% } %>
    <% if (messages.error) { %>
      <strong class="alert alert-error">
        <%= messages.error %>
      </strong>
    <% } %>
    <% if (messages.info) { %>
      <strong class="alert alert-info">
        <%= messages.info %>
      </strong>
    <% } %>
  <% } %>
</aside>
